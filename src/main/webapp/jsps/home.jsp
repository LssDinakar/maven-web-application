  $('.wpcf7-submit').each(function(){
    var parentId = $(this).parents('.wpcf7').attr('id');
    //alert(parentId);
  
  //
    var wpcf7_contact = document.querySelector( '#'+parentId );
    wpcf7_contact.addEventListener( 'wpcf7mailsent', function( event ) {
        setTimeout(function(){ 
          var mssgtxt = $('#'+parentId+' .wpcf7-response-output').text();
          $('#'+parentId+' .wpcf7-response-output').hide();

          $('#thank-you-message').text(mssgtxt);
          $('.modal').removeClass('show');
          $('.overlay').hide();
          $('#thank-you-modal').addClass('show');
          $('.overlay').show();
        }, 10);
        
        //$('#thank-you-modal').show('slow');
    }, false );
  });
